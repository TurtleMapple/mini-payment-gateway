#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/PKL/Typescript/notification-system-rabbitmq/node_modules/.pnpm/@mikro-orm+cli@6.6.6/node_modules/@mikro-orm/cli/node_modules:/mnt/d/PKL/Typescript/notification-system-rabbitmq/node_modules/.pnpm/@mikro-orm+cli@6.6.6/node_modules/@mikro-orm/node_modules:/mnt/d/PKL/Typescript/notification-system-rabbitmq/node_modules/.pnpm/@mikro-orm+cli@6.6.6/node_modules:/mnt/d/PKL/Typescript/notification-system-rabbitmq/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/PKL/Typescript/notification-system-rabbitmq/node_modules/.pnpm/@mikro-orm+cli@6.6.6/node_modules/@mikro-orm/cli/node_modules:/mnt/d/PKL/Typescript/notification-system-rabbitmq/node_modules/.pnpm/@mikro-orm+cli@6.6.6/node_modules/@mikro-orm/node_modules:/mnt/d/PKL/Typescript/notification-system-rabbitmq/node_modules/.pnpm/@mikro-orm+cli@6.6.6/node_modules:/mnt/d/PKL/Typescript/notification-system-rabbitmq/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@mikro-orm+cli@6.6.6/node_modules/@mikro-orm/cli/cli" "$@"
else
  exec node  "$basedir/../.pnpm/@mikro-orm+cli@6.6.6/node_modules/@mikro-orm/cli/cli" "$@"
fi
